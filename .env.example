# Environment Variables Example File
# Copy this file to .env and fill in your values

# MongoDB Connection String
# For MongoDB Atlas, use: mongodb+srv://username:password@cluster.mongodb.net/database
# For local MongoDB, use: mongodb://localhost:27017/aspirefurniture
MONGODB_URI=mongodb://localhost:27017/aspirefurniture

# Server Port
PORT=3002

# Node Environment
NODE_ENV=development